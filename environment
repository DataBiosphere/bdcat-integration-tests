set -a

# Resolve the location of this file and set BDCAT_HOME to the root
SOURCE="${BASH_SOURCE[0]}"
while [ -h "$SOURCE" ] ; do SOURCE="$(readlink "$SOURCE")"; done

BDCAT_HOME="$(cd -P "$(dirname "$SOURCE")" && pwd)"
